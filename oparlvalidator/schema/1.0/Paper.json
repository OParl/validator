{
    "properties": {
        "id": {
            "type": "string",
            "format": "uri"
        },
        "type": {
            "type": "string",
            "pattern": "^http://oparl\\.org/schema/1\\.0/Paper$"
        },
        "auxiliaryDocument": {
            "type": "string",
            "format": "uri",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Document"]
        },
        "body": {
            "type": "string",
            "format": "uri",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Body"]
        },
        "consultation": {
            "type": "array",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Consultation"],
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "location": {
            "type": "array",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Location"]
        },
        "mainDocument": {
            "type": "string",
            "format": "uri",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Document"]
        },
        "modified": {
            "type": "string",
            "format": "date-time"
        },
        "name": {
            "type": "string"
        },
        "originator": {
            "type": "array",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Person", "http://oparl.org/schema/1.0/Organization"]
        },
        "paperType": {
            "type": "string",
            "format": "uri"
        },
        "publishedDate": {
            "type": "string",
            "oneOf": [
                {"format": "date-time"},
                {"pattern": "^\\d{4}-((0[1-9])|(1[0-2]))-((0[1-9])|([1-2][0-9])|(3(0|1)))$"}
            ]
        },
        "reference": {
            "type": "string"
        },
        "relatedPaper": {
            "type": "string",
            "format": "uri",
            "oparl:linksTo": ["http://oparl.org/schema/1.0/Paper"]
        }
    },
    "required": [
        "id",
        "type",
        "body",
        "name",
        "mainDocument"
    ],
    "oparl:recommended": [
        "publishedDate",
        "paperType",
        "originator"
    ],
    "oparl:optional": [
        "reference",
        "relatedPaper",
        "auxiliaryDocument",
        "location",
        "consultation",
        "keyword"
    ],
    "type": "object"
}
