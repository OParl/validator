    "oparl:Paper": {
        "properties": {
            "@id": {
                "type": "string"
            },
            "auxiliaryDocument": {
                "oparl:linksTo": "oparl:Document",
                "format": "uri",
                "type": "string"
            },
            "body": {
                "format": "uri",
                "type": "string"
            },
            "consultation": {
                "type": "array",
                "items": {
                    "oparl:linksTo": "oparl:Consultation"
                }
            },
            "location": {
                "type": "array",
                "items": {
                    "oparl:linksTo": "oparl:Location"
                }
            },
            "mainDocument": {
                "format": "uri",
                "type": "string",
                "oparl:linksTo": "oparl:Document"
            },
            "modified": {
                "type": "string",
                "pattern": "^\\d+$"
            },
            "name": {
                "type": "string"
            },
            "originator": {
                "type": "array",
                "items": {
                    "oneOf": [
                        {"oparl:linksTo": "oparl:Person"},
                        {"oparl:linksTo": "oparl:Organization"}
                    ]
                }
            },
            "paperType": {
                "format": "uri",
                "type": "string"
            },
            "publishedDate": {
                "type": "string",
                "oneOf": [
                    {"format": "date-time"},
                    {"pattern": "^\\d{4}-((0[1-9])|(1[0-2]))-((0[1-9])|([1-2][0-9])|(3(0|1)))$"}
                ]
            },
            "reference": {
                "format": "uri",
                "type": "string"
            },
            "relatedPaper": {
                "format": "uri",
                "type": "string",
                "oparl:linksTo": "oparl:Paper"
            }
        },
        "required": [
            "body",
            "name",
            "mainDocument"
        ],
        "type": "object"
    },
    "oparl:Person": {
        "properties": {
            "@id": {
                "type": "string"
            },
            "@type": {
                "type": "string"
            },
            "created": {
                "type": "string",
                "format": "date-time"
            },
            "email": {
                "format": "email",
                "type": "string",
                "pattern": "^mailto:.+$"
            },
            "familyName": {
                "type": "string"
            },
            "formOfAddress": {
                "type": "string"
            },
            "gender": {
                "type": "string",
                "pattern": "^(vcard:Female)|(vcard:Male)|(vcard:None)|(vcard:Other)|(vcard:Unknown)$"
            },
            "hasMembership": {
                "items": {
                    "format": "uri",
                    "oparl:linksTo": "oparl:Membership",
                    "type": "string"
                },
                "type": "array"
            },
            "keyword": {
                "items": {
                    "type": "string"
                },
                "type": "array"
            },
            "lastModified": {
                "type": "string",
                "format": "date-time"
            },
            "locality": {
                "type": "string"
            },
            "name": {
                "type": "string"
            },
            "organization": {
                "items": {
                    "format": "uri",
                    "oparl:linksTo": "oparl:Organization",
                    "type": "string"
                },
                "type": "array"
            },
            "phone": {
                "type": "string",
                "pattern": "^tel:\\+?\\d+$"
            },
            "postalCode": {
                "type": "string"
            },
            "status": {
                "items": {
                    "type": "string"
                },
                "type": "array"
            },
            "streetAddress": {
                "type": "string"
            },
            "title": {
                "items": {
                    "type": "string"
                },
                "type": "array"
            }
        },
        "required": [
            "@type",
            "@id",
            "name"
        ],
        "type": "object"
    },
    "oparl:System": {
        "oparl:optional": [
            "website",
            "vendor",
            "product"
        ],
        "oparl:recommended": [
            "name",
            "contactEmail",
            "contactName",
            "newObjects",
            "updatedObjects",
            "removedObjects"
        ],
        "oparl:test": [
            {
                "message": "oparl:System.contactEmail must have \"mailto:\" prefix",
                "section": "5.3.1",
                "test": "oparl.validator.schema:test_must_have_mailto_prefix"
            }
        ],
        "properties": {
            "@id": {
                "type": "string"
            },
            "@type": {
                "type": "string"
            },
            "bodies": {
                "oparl:linksTo": "oparl:Body",
                "type": [
                    "string",
                    "array"
                ]
            },
            "contactEmail": {
                "format": "email",
                "type": "string"
            },
            "contactName": {
                "type": "string"
            },
            "name": {
                "type": "string"
            },
            "newObjects": {
                "format": "uri",
                "type": "string"
            },
            "oparlVersion": {
                "type": "string",
                "pattern": "^http://oparl\\.org/specs/1\\.0/$"
            },
            "product": {
                "format": "uri",
                "type": "string"
            },
            "removedObjects": {
                "format": "uri",
                "type": "string"
            },
            "updatedObjects": {
                "format": "uri",
                "type": "string"
            },
            "vendor": {
                "format": "uri",
                "type": "string"
            },
            "website": {
                "format": "uri",
                "type": "string"
            }
        },
        "required": [
            "@type",
            "@id",
            "oparlVersion",
            "bodies"
        ],
        "type": "object"
    }
}
